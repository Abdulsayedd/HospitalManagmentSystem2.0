<link href="@Url.Content("/Content/Style.css")" rel="stylesheet" type="text/css" />
@model IEnumerable<HospitalManagmentSystem2._0.Models.Payment>

@{
    ViewBag.Title = "ListPaymentReceptionist";
}

<h2>Payment List</h2>

<table class="table">
    <tr>
        <th>
            User ID
        </th>
        <th>
            Payment Amount
        </th>


        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdPayment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)$
            </td>


        </tr>
    }

</table>
