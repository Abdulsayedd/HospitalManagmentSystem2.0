<link href="@Url.Content("/Content/Style.css")" rel="stylesheet" type="text/css" />
@model IEnumerable<HospitalManagmentSystem2._0.Models.Salary>

@{
    ViewBag.Title = "ListSalarysAdministrator";
}

<h2>ListSalarysAdministrator</h2>

<p>
    @Html.ActionLink("Create New", "CreateSalaryAdministrator")
</p>
<table class="table">
    <tr>
        <th>
            Staff ID
        </th>
        <th>
            Salary
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdSalary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)$
            </td>
            <td>
                @Html.ActionLink("Edit", "EditSalaryAdministrator", new { id = item.IdSalary }) |
                @Html.ActionLink("Details", "DetailsSalaryAdministrator", new { id = item.IdSalary }) |
                @Html.ActionLink("Delete", "DeleteSalaryAdministrator", new { id = item.IdSalary })
            </td>
        </tr>
    }

</table>
