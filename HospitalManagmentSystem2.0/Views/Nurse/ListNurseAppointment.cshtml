@model IEnumerable<HospitalManagmentSystem2._0.Models.NurseAppointment>

@{
    ViewBag.Title = "ListNurseAppointment";
}

<h2>ListNurseAppointment</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NurseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AppointmentId)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NurseId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AppointmentId)
            </td>
            <td>
                @Html.ActionLink("Details", "DetailsAppointmentNurse", new { id = item.AppointmentId }) |
            </td>
        </tr>
    }

</table>
<link href="@Url.Content("/Content/Style.css")" rel="stylesheet" type="text/css" />
